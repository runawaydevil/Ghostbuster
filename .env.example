# GitHub Personal Access Token for API access
GITHUB_TOKEN=your_github_token_here

# Optional: GitHub repository for pull requests (defaults to current repo)
GITHUB_REPOSITORY=owner/repo

# Optional: Branch name for pull requests (defaults to 'update-themes')
GITHUB_BRANCH=update-themes

# Optional: Cache TTL in hours (defaults to 24)
CACHE_TTL=24

# Optional: API rate limit per hour (defaults to 5000)
RATE_LIMIT=5000